{
  "name": "@dexto/distribution-supabase",
  "version": "1.0.0",
  "description": "A Dexto distribution with Supabase blob storage and custom tools - demonstrates project-based architecture",
  "type": "module",
  "bin": {
    "dexto-supabase": "./dist/src/index.js"
  },
  "exports": {
    "./config": {
      "import": "./dist/dexto.config.js",
      "types": "./dist/dexto.config.d.ts"
    },
    "./storage/*": {
      "import": "./dist/storage/*.js",
      "types": "./dist/storage/*.d.ts"
    },
    "./tools/*": {
      "import": "./dist/tools/*.js",
      "types": "./dist/tools/*.d.ts"
    },
    "./shared/*": {
      "import": "./dist/shared/*.js",
      "types": "./dist/shared/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/src/index.js",
    "start:dev": "tsc && node dist/src/index.js",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "dexto",
    "distribution",
    "supabase",
    "blob-storage",
    "custom-tools",
    "ai-agent",
    "framework"
  ],
  "dependencies": {
    "@dexto/core": "link:../../packages/core",
    "@supabase/supabase-js": "^2.45.0",
    "yaml": "^2.8.2",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.7.0"
  },
  "files": [
    "dist",
    "agents",
    "storage",
    "tools",
    "shared",
    "src",
    "dexto.config.ts",
    "README.md",
    ".env.example"
  ]
}
