{
  "name": "@dexto/registry",
  "version": "1.6.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup && node -e \"require('fs').rmSync('tsconfig.tsbuildinfo', { force: true })\" && tsc -b tsconfig.json --emitDeclarationOnly",
    "dev": "tsup --watch",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "lint": "eslint . --ext .ts"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5"
  }
}
