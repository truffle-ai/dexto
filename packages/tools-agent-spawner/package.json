{
  "name": "@dexto/tools-agent-spawner",
  "version": "0.1.0",
  "description": "Agent spawner tools provider for Dexto agents - enables spawning and managing sub-agents",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "dexto",
    "tools",
    "agents",
    "multi-agent",
    "sub-agent",
    "spawner"
  ],
  "dependencies": {
    "@dexto/core": "workspace:*",
    "@dexto/agent-management": "workspace:*",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
