{
  "name": "@dexto/agent-config",
  "version": "1.6.7",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@dexto/core": "workspace:*",
    "@dexto/storage": "workspace:*",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.5"
  },
  "peerDependencies": {
    "zod": "^3.25.0"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 tsup && node ../../scripts/clean-tsbuildinfo.mjs && cross-env NODE_OPTIONS=--max-old-space-size=4096 tsc -b tsconfig.json --emitDeclarationOnly && node scripts/fix-dist-aliases.mjs",
    "dev": "tsup --watch",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "lint": "eslint . --ext .ts"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
