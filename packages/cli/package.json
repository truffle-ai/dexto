    "copy-agents": "tsx scripts/copy-agents.ts",
    "copy-assets": "tsx scripts/copy-assets.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@types/ws": "^8.5.11",
    "type-fest": "^4.26.1"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
