{
  "name": "@dexto/observability",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "dexto-dashboard": "./dist/bin/dexto-dashboard.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js",
      "require": "./dist/storage/index.cjs"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@dexto/core": "workspace:*",
    "@hono/node-server": "1.19.5",
    "@hono/zod-openapi": "^0.19.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^1.28.0",
    "@opentelemetry/sdk-trace-base": "^1.28.0",
    "hono": "^4.6.8",
    "zod": "^3.25.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  },
  "scripts": {
    "build": "pnpm build:lib && pnpm build:dashboard",
    "build:lib": "tsup",
    "build:dashboard": "pnpm -C dashboard build",
    "dev:dashboard": "pnpm -C dashboard dev",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
