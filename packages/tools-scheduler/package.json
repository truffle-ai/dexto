{
  "name": "@dexto/tools-scheduler",
  "version": "1.6.0",
  "description": "Scheduler tools provider for Dexto agents - enables proactive task scheduling",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "dexto",
    "tools",
    "scheduler",
    "cron",
    "automation"
  ],
  "dependencies": {
    "@dexto/agent-config": "workspace:*",
    "@dexto/core": "workspace:*",
    "cron-parser": "^4.9.0",
    "node-cron": "^4.2.1",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md"
