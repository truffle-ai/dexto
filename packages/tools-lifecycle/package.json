{
    "name": "@dexto/tools-lifecycle",
    "version": "1.6.5",
    "description": "Lifecycle and self-observation tools for Dexto agents",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup && node ../../scripts/clean-tsbuildinfo.mjs && tsc -b tsconfig.json --emitDeclarationOnly",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "keywords": [
        "dexto",
        "tools",
        "lifecycle",
        "logs",
        "memories"
    ],
    "dependencies": {
        "@dexto/agent-config": "workspace:*",
        "@dexto/core": "workspace:*",
        "zod": "^3.25.0"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.3.3",
        "vitest": "^2.1.8"
    },
    "files": [
        "dist",
        "README.md"
    ]
}
